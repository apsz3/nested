start: program

program: s_expr*

?s_expr: atom
| list

list: "(" s_expr* ")"

OP: "+" | "-" | "*" | "/" | "%" | "==" | "!=" | "<" | ">" | "<=" | ">=" | "&&" | "||" | "!" | "=" | "++" | "--" | "+=" | "-=" | "*=" | "/=" | "%=" | ">>" | "<<" | "&" | "|" | "^" | "~" | ">>>" | "<<<" | ">>=" | "<<=" | ">>>=" | "&&=" | "||=" | "&=" | "|=" | "^=" | "~=" | ">>=" | "<<=" | ">>>=" | "<<<=" | "=>" | "->" | "::" | ":" | "?" | "??"

ident: /[^\W\d-][\w\d-]*/ | OP
symbol: "#" ident

number: SIGNED_NUMBER
string: ESCAPED_STRING

?atom: ident
    | number
    | string
    | symbol



COMMENT: /;.*/

%import common.LETTER
%import common.DIGIT
%import common.SIGNED_NUMBER
%import common.ESCAPED_STRING
%import common.WS

%ignore COMMENT
%ignore WS
