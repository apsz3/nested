start: program

program: s_expr*

?s_expr: atom
| list

list: "(" s_expr* ")"

ident: /[^\W\d-][\w-]*/

number: NUMBER
string: ESCAPED_STRING

atom: ident
    | number
    | string

%import common.LETTER
%import common.DIGIT
%import common.NUMBER
%import common.ESCAPED_STRING
%import common.WS
%ignore WS
